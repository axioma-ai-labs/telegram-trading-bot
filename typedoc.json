{
    "entryPoints": [
        "src/**/*.ts"
    ],
    "entryPointStrategy": "expand",
    "out": "docs/auto",
    "tsconfig": "tsconfig.json",
    "excludePrivate": true,
    "excludeInternal": true,
    "name": "NeuroDex Bot Documentation",
    "readme": "README.md",
    "projectDocuments": [
        "docs/*.md"
    ],
    "theme": "default",
    "categorizeByGroup": true,
    "categoryOrder": [
        "Core",
        "Services",
        "Bot",
        "Utils",
        "*"
    ],
    "sort": [
        "source-order"
    ],
    "navigationLinks": {
        "GitHub": "https://github.com/axioma-ai-labs/neurodex-bot"
    },
    "searchInComments": true,
    "excludeExternals": true,
    "excludeNotDocumented": false,
    "hideGenerator": true,
    "includeVersion": true
}