ENVIRONMENT=development
# Include as described here: https://nodejs.org/en/learn/getting-started/nodejs-the-difference-between-development-and-production
NODE_ENV=production

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=

# RPC URLs (replace with your provider URLs)
ETHEREUM_MAINNET_RPC=https://your-ethereum-mainnet-rpc-url
BASE_MAINNET_RPC=https://your-base-mainnet-rpc-url
BASE_SEPOLIA_RPC=https://your-base-sepolia-rpc-url
BNC_RPC=https://your-bnc-rpc-url

DEFAULT_SLIPPAGE=1   # Default Slippage (percentage)
DEFAULT_FEE=1   # Default free (percentage)
DEFAULT_FEE_WALLET=
DEFAULT_GAS_PRIORITY=medium   # Default Gas Priority 'standard' | 'fast' | 'instant'

# Prisma Database URI
# For more info: https://www.prisma.io/docs/getting-started/quickstart-prismaPostgres
DATABASE_URL="prisma+postgres://accelerate.prisma-data.net/?api_key="
# Database Encryption key. Must be generated with cloak!
PRISMA_FIELD_ENCRYPTION_KEY=""

COVALENTHQ_API_KEY=""

BETTERSTACK_SOURCE_TOKEN=""
BETTERSTACK_ENDPOINT=""

# Supabase (for secure private key storage)
# Get these from your Supabase project settings: https://app.supabase.com/
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Encryption keys
MASTER_ENCRYPTION_PASSWORD=
WALLET_ENCRYPTION_KEY=